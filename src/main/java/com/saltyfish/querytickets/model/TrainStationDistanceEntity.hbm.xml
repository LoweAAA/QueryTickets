<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.saltyfish.querytickets.model.TrainStationDistanceEntity" table="trainStationDistance"
           schema="trainServer">

        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="native"/>
        </id>

        <many-to-one name="start" class="com.saltyfish.querytickets.model.TrainStationEntity" lazy="false">
            <column name="id"/>
        </many-to-one>

        <many-to-one name="end" class="com.saltyfish.querytickets.model.TrainStationEntity" lazy="false">
            <column name="id"/>
        </many-to-one>

        <property name="distance" type="java.lang.Double">
            <column name="distance" precision="-1" not-null="true"/>
        </property>

    </class>
</hibernate-mapping>