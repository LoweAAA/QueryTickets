<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.saltyfish.querytickets.model.TrainPassByStationsEntity" table="trainPassByStations"
           schema="trainServer">
        <id name="id" column="id"/>
        <property name="trainNumber" column="trainNumber"/>
        <property name="stationId" column="stationId"/>
        <property name="time" column="time"/>
        <property name="type" column="type"/>
        <property name="money" column="money"/>
        <property name="distance" column="distance"/>
    </class>
</hibernate-mapping>